{
	"name": "danwe/dataproviders-differenttypesvalues",
	"version": "1.0.0",
	"type": "library",
	"description": "PhpUnit compatible data provider providing one value of each of PHP's built-in types per test case. Allows to exclude types based on the name of the test using the data provider.",
	"keywords": [
		"data provider",
		"values",
		"PhpUnit",
		"test",
		"helper"
	],
	"homepage": "https://github.com/DanweDE/php-DataProviders-DifferentTypesValues",
	"license": "MIT License",
	"authors": [
		{
			"name": "Daniel A. R. Werner",
			"email": "daniel.a.r.werner@gmail.com",
			"homepage": "https://www.mediawiki.org/wiki/User:Danwe",
			"role": "developer"
		}
	],
	"require": {
		"php": ">=5.3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "4.6.*"
	},
	"autoload": {
		"psr-4": {
			"Danwe\\DataProviders\\": "src/",
			"Danwe\\DataProviders\\Tests\\": "tests/"
		}
	}
}
